{
	"name" : "mongator/mongator",
	"type" : "library",
	"description" : "Mongator is a easy, powerful and ultrafast ODM for PHP and MongoDB. (forked from Mandango project)",
	"keywords" : [
		"odm",
		"mongodb"
	],
	"homepage" : "http://mongator.com",
	"license" : "MIT",
	"authors" : [{
			"name" : "Máximo Cuadros",
			"email" : "mcuadros@gmail.com"
		}, {
			"name" : "Daniel Ripollés",
			"email" : "daniel@yunait.com"
		}, {
			"name" : "Pablo Díez",
			"email" : "pablodip@gmail.com"
		}
	],
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/srggroup/mondator"
		}
	],
	"require" : {
		"php" : "^7",
		"mongator/mondator" : "^2.0",
		"symfony/event-dispatcher" : "^2.8||~3.0||~4.0",
		"mongodb/mongodb" : "^1.1"
	},
	"require-dev" : {
		"athletic/athletic" : "dev-master",
		"pagerfanta/pagerfanta": "^1.1.0|^2.0.0"
	},
	"suggest" : {
		"pagerfanta/pagerfanta" : "Allow paginating mongator documents"
	},
	"autoload" : {
		"psr-0" : {
			"Mongator\\" : "src/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "2.x-dev"
		}
	}
}